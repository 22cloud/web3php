{
    "name": "IlyasDeckers/web3php",
    "description": "A simple PHP package to interact with the Ethereum web3 API",
    "keywords": ["laravel", "web3", "ethereum"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ilyas Deckers",
            "email": "ilyas@phasehosting.io"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "~5.0",
        "illuminate/session": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "IlyasDeckers\\Web3PHP\\": "src"
        }
    },
    "require-dev": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "IlyasDeckers\\Web3PHP\\Web3ServiceProvider"
            ],
            "aliases": {
                "Alert": "IlyasDeckers\\Web3PHP\\Web3PHP"
            }
        }
    }
}
