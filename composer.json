    {
    "name": "ilyasdeckers/web3php",
    "description": "A simple PHP package to interact with the Ethereum web3 API",
    "keywords": ["laravel", "web3", "ethereum"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ilyas Deckers",
            "email": "ilyas@phasehosting.io"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "~5.0",
        "illuminate/session": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "IlyasDeckers\\Web3PHP\\": "packages/Web3PHP/src/web3php"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IlyasDeckers\\Web3PHP\\Providers\\Web3PHPServiceProvider"
            ],
            "aliases": {
                "Web3PHP": "IlyasDeckers\\Web3PHP\\src\\web3php\\Ethereum"
            }
        }
    }
}
